{
    "presets": [
        "es2015",
        "react",
        "stage-0"
    ],
    "plugins": [
        ["transform-es2015-modules-commonjs", {
            "loose": true
        }],
        ["transform-runtime", {
            "polyfill": false,
            "regenerator": true
        }],
       "transform-decorators-legacy",
       "transform-es3-property-literals",
       "transform-es3-member-expression-literals"
    ]
}
